@page
@model lms.Pages.IssuedBookModel
@{
     ViewData["title"] = "ISSUED BOOK";
}


<h1>LIST OF USERS</h1>


<table class="table">
    <thead>
        <tr>
            <th>USER NAME</th>
            <th>BOOK ID</th>
             <th>ISSUED DATE</th>
            <th>RETURN DATE</th>
             
             
        </tr>
        
    </thead>
    <tbody>
        @foreach(var buser in Model.listofissuedbook )
        {
            <tr>
                <td>@buser.name</td>
                <td>@buser.bid</td>
                <td>@buser.idatef</td>
                <td>@buser.rdatef</td>
                
                
               
                
           </tr>
        }
    </tbody>
</table>
 <div class="row mb-3"> 
        <div class="offset-sm-3 col-sm-3 d-grid">
              <a class=" btn btn-primary"  href="/admin">Go Back</a>
   
        </div>
     </div>
